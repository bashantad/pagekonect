<div style="font-weight:bold;"> 
	<% if row.url.present? || row.facebook_url.present? || row.google_plus_url.present? || row.twitter_url.present? %>
		LINKS FOR THIS	
	<% end %>
</div>
<%=link_to "W", show_url_pages_path(:url => row.url), :class => "fa fa-2x" if row.url.present? && params[:controller] != "videos"%> 
<%=link_to "<span class='fa fa-2x fa-facebook-square'><span>".html_safe, show_url_pages_path(:url => row.facebook_url) if row.facebook_url.present?%>
<%=link_to "<span class='fa fa-2x fa-google-plus-square'><span>".html_safe, show_url_pages_path(:url => row.google_plus_url) if row.google_plus_url.present?%>
<%=link_to "<span class='fa fa-2x fa-twitter-square'><span>".html_safe, show_url_pages_path(:url => row.twitter_url) if row.twitter_url.present?%>
